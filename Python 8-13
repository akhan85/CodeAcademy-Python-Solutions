# Python 8/13
# Keeping Track of the Produce

prices = {
    "banana" : 4,
    "apple" : 2,
    "orange" : 1.5,
    "pear" : 3
}

stock = {
    "banana" : 6,
    "apple" : 0,
    "orange" : 32,
    "pear" : 15
}

for key in prices:
    print key
    print "price: %r" % prices[key]
    print "stock: %r" % stock[key]
